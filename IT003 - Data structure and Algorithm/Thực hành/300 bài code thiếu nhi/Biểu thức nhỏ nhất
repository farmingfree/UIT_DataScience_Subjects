#include <iostream>
#include <vector>
#include <cmath>
using namespace std;

int main() {
    long long t; 
    cin >> t;

    while (t--) {
        long long n; 
        cin >> n;

        vector<long long> a(n); 
        long long sum = 0; 
        for (int i = 0; i < n; i++) {
            cin >> a[i];
            sum += a[i];
        }

       
        int x = sum / n;

       
        if (abs(sum - n * x) > abs(sum - n * (x + 1))) {
            x++;
        }

        cout << x << endl;
    }

    return 0;
}
