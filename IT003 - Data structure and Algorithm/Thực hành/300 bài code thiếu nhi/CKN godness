
#include<iostream>
#include<stack>
#include<vector>
using namespace std;
int a[100];
void in() {
        for (int i = 1; i <= a[0]; i++)
        cout << a[i];
        cout << " ";
}
int n, k;
int count=0;
int final = 0;
void sinh() {
    int i = k;
    while (i >= 1 && a[i] == n - k + i)
        i--;
    if (i == 0) {
        final = 1;
    }
    else {
        a[i]++;
        for (int j = i + 1; j <= k; j++) {
            a[j] = a[j - 1] + 1; 
        }
    }
}
int main() {
    cin >> n>> k;
    for(int i=1;i<=k;i++){
        a[i]=i;
    }
    while (!final) {
        count++;
        if(count%1000==0)
        {for (int i = 1; i <= k; i++) cout << a[i] << " ";
        cout << endl;}
        sinh();
    }
    
    return 0;
}
